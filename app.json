{
  "expo": {
    "name": "Cloudtec Gym",
    "slug": "ct-gym",
    "scheme": "ct-gym",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/PNG/ios-dark_icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/PNG/ios-dark_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#253649"
    },
    "ios": {
      "supportsTablet": true,
      "icon": {
        "dark": "./assets/PNG/ios-dark_icon.png",
        "light": "./assets/PNG/ios-dark_icon.png",
        "tinted": "./assets/PNG/ios-tinted_icon.png"
      },
      "bundleIdentifier": "com.nvourlidas.ctgym",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/PNG/android_icon.png",
        "backgroundColor": "#253649"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.nvourlidas.ctgym",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-notifications"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b7e9195c-57aa-4434-8613-228f6f9fe1a3"
      }
    },
    "owner": "nvourlidas",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/b7e9195c-57aa-4434-8613-228f6f9fe1a3"
    }
  }
}
